{
  "name": "patternjournal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start-dev": "NODE_ENV=development ./node_modules/.bin/nodemon --legacy-watch --inspect=0.0.0.0 ./bin/www",
    "test": "NODE_ENV=test npm run migrate:reset && jest --coverage --forceExit",
    "test-watch": "jest --watch",
    "migrate": "sequelize-cli db:migrate",
    "migrate:reset": "sequelize-cli db:migrate:undo:all && npm run migrate",
    "prettify": "./node_modules/.bin/prettier --write '**/*.js' '**/*.md' '**/*.yml' "
  },
  "dependencies": {
    "bulma": "^0.7.5",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "express-session": "^1.16.2",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^7.12.1",
    "pug": "2.0.0-beta11",
    "sequelize": "^5.18.1",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  }
}
